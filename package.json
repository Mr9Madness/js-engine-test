{
  "name": "napi-test",
  "version": "1.0.0",
  "main": "index.js",
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "start": "node main.js",
    "start:win": "node.exe main.js",
    "build": "nr build:linux && nr build:win",
